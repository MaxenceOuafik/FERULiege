@article{sell2022,
  title = {Multi {{Inter}} and {{Transdisciplinarity}} within the {{Public Health Workforce A Scoping Review}} to {{Assess Definitions}} and {{Applications}} of {{Concepts}}},
  author = {Sell, Kerstin and Hommes, Franziska and Fischer, Florian and Arnold, Laura},
